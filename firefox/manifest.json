{
	"name": "Ryne Toolkit",
	"description": "Adds quality of life features to ryne's humanizer and chat web pages. Better code formatting, Auto focus, Cleaner style, and more!",
	"version": "2.0.0",
	"manifest_version": 2,
	"browser_specific_settings": {
		"gecko": {
			"id": "ryne-batch-delete@bennnnzo.com"
		}
	},
	"permissions": [
		"storage",
		"tabs",
		"https://ryne.ai/*"
	],
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"browser_action": {
		"default_popup": "/popup/index.html"
	},
	"background": {
		"scripts": ["background.js"]
	}
}