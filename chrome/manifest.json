{
	"name": "Ryne Toolkit",
	"description": "Adds quality of life features to ryne's humanizer and chat web pages. Better code formatting, Auto focus, Cleaner style, and more!",
	"version": "2.0.0",
	"manifest_version": 3,
	"permissions": [
		"scripting",
		"storage",
		"tabs"
	],
	"host_permissions": [
		"https://ryne.ai/*"
	],
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"action": {
		"default_popup": "/popup/index.html"
	},
	"background": {
		"service_worker": "background.js"
	}
}